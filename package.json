{
  "name": "@es-joy/jsdoc-eslint-parser",
  "version": "0.9.0",
  "author": "Brett Zamir <brettz9@yahoo.com>",
  "contributors": [],
  "description": "Parser to support JavaScript with JSDoc comments and types available as AST",
  "license": "MIT",
  "keywords": [
    "eslint-parser",
    "jsdoc"
  ],
  "main": "src/index.js",
  "browserslist": [
    "cover 100%"
  ],
  "scripts": {
    "lint-test": "echo \"Getting \\`Cannot read property 'start' of undefined\\` errors without no-eslintrc but this works at least\" && eslint --no-eslintrc --config .eslintrc-test.js test/fixtures",
    "eslint": "eslint --ext=jd,md,html .",
    "lint": "npm run eslint --",
    "mocha": "mocha",
    "test": "nyc npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/es-joy/jsdoc-eslint-parser.git"
  },
  "bugs": {
    "url": "https://github.com/es-joy/jsdoc-eslint-parser/issues"
  },
  "homepage": "https://github.com/es-joy/jsdoc-eslint-parser",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@brettz9/eslint-plugin": "^1.0.4",
    "eslint-config-ash-nazg": "32.13.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.1.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-no-unsanitized": "^4.0.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@es-joy/jsdoccomment": "0.22.1",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.13.0",
    "esquery": "^1.4.0",
    "typescript": "^4.6.3"
  }
