{
  "name": "@es-joy/jsdoc-eslint-parser",
  "version": "0.24.0",
  "author": "Brett Zamir <brettz9@yahoo.com>",
  "contributors": [],
  "description": "Parser to support JavaScript with JSDoc comments and types available as AST",
  "license": "MIT",
  "keywords": [
    "eslint-parser",
    "jsdoc"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    },
    "./typescript.js": {
      "types": "./dist/typescript.d.ts",
      "import": "./typescript.js",
      "require": "./dist/typescript.cjs"
    }
  },
  "c8": {
    "checkCoverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "prepublishOnly": "pnpm i && pnpm rollup && pnpm tsc",
    "tsc": "tsc",
    "rollup": "rollup -c",
    "lint-test": "eslint --config eslint.config-test.js test/fixtures",
    "eslint": "eslint .",
    "lint": "npm run eslint --",
    "mocha": "mocha --reporter-option maxDiffSize=10000000",
    "test": "c8 npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/es-joy/jsdoc-eslint-parser.git"
  },
  "bugs": {
    "url": "https://github.com/es-joy/jsdoc-eslint-parser/issues"
  },
  "homepage": "https://github.com/es-joy/jsdoc-eslint-parser",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/esquery": "^1.5.4",
    "@types/estree": "^1.0.8",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "eslint-config-ash-nazg": "39.5.0",
    "eslint-plugin-jsdoc": "^60.8.0",
    "mocha": "^11.7.4",
    "object-deep-compare": "^2.0.0",
    "rollup": "^4.52.4"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@es-joy/jsdoccomment": "0.67.2",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "esquery": "^1.6.0",
    "typescript": "^5.9.3"
  }
}
