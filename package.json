{
  "name": "@es-joy/jsdoc-eslint-parser",
  "version": "0.21.1",
  "author": "Brett Zamir <brettz9@yahoo.com>",
  "contributors": [],
  "description": "Parser to support JavaScript with JSDoc comments and types available as AST",
  "license": "MIT",
  "keywords": [
    "eslint-parser",
    "jsdoc"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    },
    "./typescript.js": {
      "types": "./dist/typescript.d.ts",
      "import": "./typescript.js",
      "require": "./dist/typescript.cjs"
    }
  },
  "c8": {
    "checkCoverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "browserslist": [
    "cover 100%"
  ],
  "scripts": {
    "prepublishOnly": "pnpm i && pnpm rollup && pnpm tsc",
    "tsc": "tsc",
    "rollup": "rollup -c",
    "lint-test": "echo \"Getting \\`Cannot read property 'start' of undefined\\` errors without no-eslintrc but this works at least\" && eslint --no-eslintrc --config eslint.config-test.js test/fixtures",
    "eslint": "eslint .",
    "lint": "npm run eslint --",
    "mocha": "mocha --require chai/register-expect.js --reporter-option maxDiffSize=10000000",
    "test": "c8 npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/es-joy/jsdoc-eslint-parser.git"
  },
  "bugs": {
    "url": "https://github.com/es-joy/jsdoc-eslint-parser/issues"
  },
  "homepage": "https://github.com/es-joy/jsdoc-eslint-parser",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/eslint": "^9.6.1",
    "@types/esquery": "^1.5.4",
    "@types/estree": "^1.0.6",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "eslint-config-ash-nazg": "36.19.0",
    "eslint-plugin-jsdoc": "^50.3.1",
    "mocha": "^10.7.3",
    "rollup": "^4.24.0"
  },
  "dependencies": {
    "@babel/core": "^7.25.7",
    "@babel/eslint-parser": "^7.25.7",
    "@es-joy/jsdoccomment": "0.48.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "esquery": "^1.6.0",
    "typescript": "^5.6.2"
  }
}
